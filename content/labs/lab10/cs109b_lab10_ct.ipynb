   "source": [
    "# hyper-parameters\n",
    "NOISE_DIM = 10\n",
    "DATA_DIM = 1\n",
    "G_LAYERS = 1\n",
    "D_LAYERS = 1"
   ]
  },
  {
   "cell_type": "code",
